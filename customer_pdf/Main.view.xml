<core:View xmlns:core="sap.ui.core"
           xmlns="sap.m"
           xmlns:l="sap.ui.layout"
           xmlns:f="sap.ui.layout.form"
           controllerName='customer_pdf.Main'>

    <Page title="Empleado">
        <content>
            <f:SimpleForm id="SimpleFormChange471"
                          minWidth="1024"
                          maxContainerCols="1"
                          editable="true"
                          layout="ResponsiveGridLayout"
                          title="Datos"
                          labelSpanL="3"
                          labelSpanM="3"
                          emptySpanL="4"
                          emptySpanM="4"
                          columnsL="1"
                          columnsM="1">
                <f:content>
                    <Label text="Cedula" />
                     <Input value="" >
                         <layoutData>
                                <l:GridData  span="L2 M2 S8" />
                            </layoutData>
                    </Input>
                    <Label text="Fecha" labelFor="DP1"/>
                    <DatePicker
                            id="DP1"
                            value="2014-03-26" valueFormat="yyyy-MM-dd"
                            change="handleChange"
                            class="sapUiSmallMarginBottom" >
                    </DatePicker>
                      <Button text="Consultar">
                        <layoutData>
                            <l:GridData indent="L3 M3" span="L2 M2 S6" linebreak="true"/>
                        </layoutData>
                    </Button>

                </f:content>
            </f:SimpleForm>
        </content>
    </Page>
</core:View>
